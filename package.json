{
  "name": "rxjs-trading-bot",
  "version": "0.0.1",
  "engines": {
    "node": "8.2.1"
  },
  "description": "RxJS Trading bot",
  "scripts": {
    "test:staged": "jest",
    "lint:staged": "lint-staged",
    "start": "node dist/index.js",
    "dev": "npm run build && npm run build:watch | nodemon --watch dist/ dist/index.js",
    "test": "NODE_ENV=test jest --watch --coverage",
    "build:watch": "webpack --mode development --config webpack.config.js --watch",
    "build": "webpack --mode production --config webpack.config.js"
  },
  "author": "Kenneth Christensen",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "lint-staged": "^7.1.0",
    "nodemon": "^1.13.3",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.23.0",
    "chalk": "^2.4.1",
    "moment": "^2.20.1",
    "node-bittrex-api": "^0.8.1",
    "pg": "^7.4.1",
    "pre-commit": "^1.2.2",
    "progress": "^2.0.0",
    "rxjs": "^5.5.5",
    "talib": "^1.0.3"
  },
  "pre-commit": [
    "lint:staged",
    "test:staged"
  ]
}
